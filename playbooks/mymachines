# mek
[workstation]
localhost ansible_connection=local ansible_python_interpreter=/usr/bin/python2

# Jezis, ja pouzivam fakt debilni hostname

[brepta]
192.168.2.1 ansible_connection=ssh ansible_ssh_user=root ansible_ssh_port=2222

[malina]
malina.local ansible_python_interpreter=/usr/bin/python2

[oblacek]
oblacek.stibi.name ansible_python_interpreter=/usr/bin/python2
